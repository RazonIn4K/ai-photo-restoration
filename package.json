{
  "name": "face-restore-ai",
  "version": "1.0.0",
  "description": "AI Photo Restoration Service",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "canary:smoke": "tsx scripts/run-canary.ts smoke",
    "canary:meta": "tsx scripts/run-canary.ts meta",
    "canary:visual": "tsx scripts/run-canary.ts visual"
  },
  "keywords": [
    "ai",
    "photo",
    "restoration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@simplewebauthn/server": "^13.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exiftool-vendored": "^31.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "mongodb": "^6.3.0",
    "mongodb-client-encryption": "^6.1.0",
    "mongoose": "^8.3.1",
    "pino": "^9.14.0",
    "pino-pretty": "^10.3.1",
    "rate-limit-redis": "^4.2.3",
    "redis": "^5.9.0",
    "sharp": "^0.34.4",
    "sharp-phash": "^2.2.0",
    "ulid": "^2.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "audit-ci": "^7.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^2.1.8"
  }
}
