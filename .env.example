# Application
NODE_ENV=development
PORT=4000
LOG_LEVEL=info

# Database & Queue
MONGO_URI=mongodb://admin:changeme@localhost:27017/face_restore?authSource=admin
MONGO_MAX_POOL_SIZE=20
MONGO_MIN_POOL_SIZE=5
MONGO_CONNECT_TIMEOUT_MS=10000
MONGO_SERVER_SELECTION_TIMEOUT_MS=5000
MONGO_HEARTBEAT_FREQUENCY_MS=10000
MONGO_KEY_VAULT_NAMESPACE=encryption.__keyVault
MONGO_CSFLE_KMS_PROVIDER=local
MONGO_DISABLE_CSFLE=false
MONGO_CRYPT_SHARED_LIB_PATH=
# Base64 encoded 96-byte key generated via `openssl rand -base64 96`
MONGO_LOCAL_MASTER_KEY_BASE64=
# AWS provider settings (required when MONGO_CSFLE_KMS_PROVIDER=aws)
MONGO_AWS_KMS_KEY_ID=
MONGO_AWS_KMS_ACCESS_KEY_ID=
MONGO_AWS_KMS_SECRET_ACCESS_KEY=
MONGO_AWS_KMS_REGION=
REDIS_URL=redis://localhost:6379

# Observability
METRICS_PORT=9464

# Docker defaults
MONGO_ROOT_USER=admin
MONGO_ROOT_PASSWORD=changeme
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=changeme
